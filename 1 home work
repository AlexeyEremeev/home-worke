#1
python:
s = [i for i in range(0, 100, 1)]
a = (input())
for i in range(100):
	if (a == s[i]):
		print(i)
    
c++:
#include <iostream>

using namespace std;
int main(int argc, char *argv[]) {
	int a[100];
	int k;
	cin >> k;
	for (int i = 0; i < 100; i ++)
		a[i] = i;
	for (int i = 0; i < 100; i ++)
		if (k == a[i])
			cout << i;
}

